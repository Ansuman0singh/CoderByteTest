//Using single inheritence



class Address {

    

String addressLine;

    

String city;

    

String zip;

}

 

 

class NaturalPerson extends Address

{

    

Integer id;

    

String name;

    

Date dateOfBirth;

    

Integer age;

    

//Address address;

}

 

 

class LegalPerson extends Address

{ 

    

Integer id;

    

String name;

    

Integer numberOfEmployees;

    

//Address address;

}
